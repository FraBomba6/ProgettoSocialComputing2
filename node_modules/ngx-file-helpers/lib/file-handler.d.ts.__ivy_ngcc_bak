import { EventEmitter } from '@angular/core';
import { ReadFile } from './read-file';
import { ReadMode } from './read-mode.enum';
export declare abstract class FileHandler {
    readMode: ReadMode;
    filter: (file: File, index: number, files: Array<File>) => boolean;
    readStart: EventEmitter<number>;
    readEnd: EventEmitter<number>;
    protected readFiles(files: FileList, onFileRead: (fileRead: ReadFile) => void): Promise<void>;
}
